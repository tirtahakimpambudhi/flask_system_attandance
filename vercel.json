{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/students/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/absence/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "buildCommand": "./build.sh"
  }
  