{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "includeFiles": [
            "requirements.txt"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/students/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/absence/(.*)",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/app.py"
      }
    ],
    "build": {
      "env": {
        "PIP_NO_CACHE_DIR": "off",
        "PIP_DISABLE_PIP_VERSION_CHECK": "on"
      },
      "command": "apt-get update && apt-get install -y cmake && pip3.12 install --disable-pip-version-check --target . --upgrade -r /vercel/path0/requirements.txt"
    }
  }
  